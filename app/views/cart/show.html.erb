
<h1>Cart Details</h1>

<% if @cart.cart_items.any? %>
  <ul>
    <% @cart.cart_items.each do |item| %>
      
    <strong>Name:</strong>
    <%= item.product.name %>
    <strong>Description:</strong>
    <%= item.product.desc %>
    <strong>Price:</strong>
    <%= item.product.price %>


    <% end %>
  </ul>
<% else %>
  <p>Your cart is empty.</p>
<% end %>

